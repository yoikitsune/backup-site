# Configuration de l'environnement de test production
# Copie ce fichier en .env et modifie les versions selon tes besoins

# Versions
PHP_VERSION=8.2
MYSQL_VERSION=8.0
WORDPRESS_VERSION=6.4

# Accès MySQL
MYSQL_ROOT_PASSWORD=root
MYSQL_DATABASE=wordpress
MYSQL_USER=wordpress
MYSQL_PASSWORD=wordpress

# Accès SSH (pour les sauvegardes)
SSH_USER=testuser
SSH_PASSWORD=testpass

# Ports
WORDPRESS_PORT=80
MYSQL_PORT=3306
SSH_PORT=2222
